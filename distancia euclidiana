#include<stdio.h>
#include<stdlib.h>
#include<math.h>


void retorna(int x1, int x2, int y1, int y2){
	
	int distancia;
	
	distancia = sqrt(pow((x1 - x2),2)+ pow((y1 - y2),2));
	
	printf("\n distancia = %d",distancia);
	
}

int main(){
	
	int x1,x2,y1,y2;
	
	printf("\n entre com x1:");
	scanf("%d",&x1);
	printf("\n entre com x2:");
	scanf("%d",&x2);
	printf("\n entre com y1:");
	scanf("%d",&y1);
	printf("\n entre com y2:");
	scanf("%d",&y2);
	
	retorna(x1,x2,y1,y2);
}
